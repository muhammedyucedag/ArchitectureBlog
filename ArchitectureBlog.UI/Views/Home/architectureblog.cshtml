@model HomePageProjectModel
<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Optima</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- <link rel="manifest" href="site.webmanifest"> -->
    <link rel="shortcut icon" type="image/x-icon" href="~/ArchitectureTemplate-1/img/favicon.png">
    <!-- Place favicon.ico in the root directory -->
    <!-- CSS here -->
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/magnific-popup.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/themify-icons.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/nice-select.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/flaticon.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/animate.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/slicknav.css">
    <link rel="stylesheet" href="~/ArchitectureTemplate-1/css/style.css">
    <!-- <link rel="stylesheet" href="css/responsive.css"> -->
</head>

<body>

    
    @await Html.PartialAsync("header")

    <div class="slider_area">
        <div class="single_slider overlay2 d-flex align-items-center justify-content-center slider_bg_1">
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="slider_text text-center">
                            <h3>Mimari vizyonunuzu gerçeğe dönüştürmek için hazırız</h3>
                            <p>Birlikte hayallerinizi inşa edin. Mimarlıkta uzman ekibimizle tanışın</p>
                            <a href="#ourprojects" class="boxed-btn2">
                                Projelerimizi görün
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="about_area" id="about">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-md-6">
                    <div class="about_thumb">
                        <img src="~/ArchitectureTemplate-1/img/about/1.png" alt="">
                        <div class="exprience">
                            <h1>25</h1>
                            <span>Yılların Deneyimi</span>
                        </div>
                    </div>
                </div>
                <div class="col-xl-5 offset-xl-1 col-md-6" >
                    <div class="about_info">
                        <div class="section_title">
                            <span class="sub_heading">Hakkımızda</span>
                            <h3>
                                Mimari plan tasarla<br>
                                ve inşa et
                            </h3>
                            <div class="seperator"></div>
                        </div>
                        <p>
                            Farklı beklentilere ve ihtiyaçlara cevap veren, özgün mimari tasarımlar için doğru yerdesiniz. Mimarlıkta uzman ekibimizle hayallerinizi gerçekleştirmek için buradayız. Yenilikçi yaklaşımımızla müşterilerimizin memnuniyetini her zaman en üst seviyede tutuyoruz. Projelerimizi inceleyerek işimizin kalitesine ve özgünlüğüne yakından tanık olabilirsiniz. Bizimle çalışmak için hemen iletişime geçin.
                        </p>
                        <ul class="about_list">
                            <li>Mekanlarınızda özgünlük yaratın.</li>
                            <li>Estetiği işlevsel kılın. </li>
                            <li>Yaratıcı mimari çözümler. </li>
                            <li>Mimari tasarımlarınızın partneri. </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="dream_service" id="service">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="section_title text-center mb-95">
                        <span class="sub_heading">Servislerimiz</span>
                        <h3>Hayalinizi Bizimle Gerçekleştirin</h3>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-4 col-md-4">
                    <div class="single_dream text-center">
                        <div class="thumb">
                            <img src="~/ArchitectureTemplate-1/img/dream/1.png" alt="">
                        </div>
                        <h3>İç Mekan</h3>
                        <p>
                            Consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna
                            aliqua quis ipsum suspendisse.
                        </p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_dream text-center">
                        <div class="thumb">
                            <img src="~/ArchitectureTemplate-1/img/dream/2.png" alt="">
                        </div>
                        <h3>Dış</h3>
                        <p>
                            Consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna
                            aliqua quis ipsum suspendisse.
                        </p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_dream text-center">
                        <div class="thumb">
                            <img src="~/ArchitectureTemplate-1/img/dream/3.png" alt="">
                        </div>
                        <h3>Çevre</h3>
                        <p>
                            Consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna
                            aliqua quis ipsum suspendisse.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
 
    <div class="lastest_project">
        <div class="container">
            <div class="row" id="ourprojects">
                <div class="col-xl-12">
                    <div class="section_title mb-60">
                        <span class="sub_heading">Proje Görünümü</span>
                        <h3>
                            Son Projelerimiz
                        </h3>
                        <div class="seperator"></div>
                    </div>
                </div>
            </div>
            @foreach (var item in Model.Projects)
            {
                <div class="row align-items-center mb-80">
                    <div class="col-xl-6 col-md-6">
                        <div class="single_project_thumb">
                            <img width="585" height="450" src="@item.Images.FirstOrDefault().Url" alt="">
                        </div>
                    </div>
                    <div class="col-xl-5 offset-xl-1 col-md-6">
                        <div class="section_title">
                            <span class="sub_heading2">@item.Location</span>
                            <h4>@item.Name</h4>
                            <p>
                                @item.Description
                            </p>
                            <a href="/Home/ArchitectureBlogDetail/" class="boxed-btn">Daha Fazla Göster</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    
    @await Html.PartialAsync("footer")
 
    <script src="~/ArchitectureTemplate-1/js/vendor/modernizr-3.5.0.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/popper.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/bootstrap.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/owl.carousel.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/isotope.pkgd.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/ajax-form.js"></script>
    <script src="~/ArchitectureTemplate-1/js/waypoints.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.counterup.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/imagesloaded.pkgd.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/scrollIt.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.scrollUp.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/wow.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/nice-select.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.slicknav.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.magnific-popup.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/plugins.js"></script>

    <!--contact js-->
    <script src="~/ArchitectureTemplate-1/js/contact.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.form.js"></script>
    <script src="~/ArchitectureTemplate-1/js/jquery.validate.min.js"></script>
    <script src="~/ArchitectureTemplate-1/js/mail-script.js"></script>

    <script src="~/ArchitectureTemplate-1/js/main.js"></script>

</body>

</html>